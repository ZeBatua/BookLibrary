select * from member;
select * from book;

ALTER SEQUENCE member_member_id_seq RESTART;
ALTER SEQUENCE book_book_id_seq RESTART;
UPDATE book SET book_id = DEFAULT WHERE 1=1;
UPDATE member SET member_id = DEFAULT WHERE 1=1;


